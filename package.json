{
  "name": "ai-speedometer",
  "version": "1.3.7",
  "description": "A comprehensive CLI tool for benchmarking AI models across multiple providers with parallel execution and professional metrics",
  "main": "cli.js",
  "bin": {
    "ai-speedometer": "dist/ai-speedometer",
    "aispeed": "dist/ai-speedometer"
  },
  "preferGlobal": true,

  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node cli.js",
    "cli": "node cli.js",
    "cli:debug": "node cli.js --debug",
    "build": "esbuild cli.js --bundle --platform=node --outfile=dist/ai-speedometer --format=cjs --minify --external:jsonc-parser --external:dotenv && sed -i '1s|^.*|#!/usr/bin/env node|' dist/ai-speedometer",
    "build:dev": "esbuild cli.js --bundle --platform=node --outfile=dist/ai-speedometer --format=cjs --external:jsonc-parser --external:dotenv && sed -i '1s|^.*|#!/usr/bin/env node|' dist/ai-speedometer",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ai",
    "benchmark",
    "cli",
    "speedometer",
    "performance",
    "llm",
    "models",
    "testing",
    "openai",
    "anthropic",
    "ai-sdk",
    "parallel",
    "metrics"
  ],
  "author": "ai-speedometer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aptdnfapt/Ai-speedometer.git"
  },
  "bugs": {
    "url": "https://github.com/aptdnfapt/Ai-speedometer/issues"
  },
  "homepage": "https://github.com/aptdnfapt/Ai-speedometer#readme",
  "files": [
    "dist/ai-speedometer",
    "docs/",
    "README.md",
    "ai-benchmark-config.json.template"
  ],
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.17",
    "@ai-sdk/openai-compatible": "^1.0.17",
    "ai": "^5.0.44",
    "cli-table3": "^0.6.5",
    "dotenv": "^17.2.2",
    "jsonc-parser": "^3.3.1"
  },
  "devDependencies": {
    "esbuild": "^0.25.10"
  }
}
